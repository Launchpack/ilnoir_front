Today() {\r\n        let today = moment().format('YY-DD-MM');\r\n        this.$store.commit('setHidePopup', today);\r\n        this.$store.commit('setExposuredPopup', true);\r\n        this.$refs.popupModal.close();\r\n      },\r\n      offPopup() {\r\n        this.$store.commit('setExposuredPopup', true);\r\n        this.$refs.popupModal.close();\r\n      },\r\n      closeModal() {\r\n        this.$store.commit('setExposuredPopup', true);\r\n      }\r\n    },\r\n    computed: {\r\n      popupStyle() {\r\n        return {\r\n          height: `${320*4/3}px`\r\n        }\r\n      }\r\n    }\r\n  }\r\n</script>\r\n\r\n<style lang=\"stylus\" type=\"stylus\" scoped>\r\n\r\n  .close-area\r\n    width 50%\r\n    padding 12px\r\n    text-align center\r\n</style>\r\n<style lang=\"stylus\">\r\n  .popup .sweet-modal.is-alert .sweet-content\r\n    padding 0 !important\r\n</style>"],"sourceRoot":"src/components/Components/Common"}]}