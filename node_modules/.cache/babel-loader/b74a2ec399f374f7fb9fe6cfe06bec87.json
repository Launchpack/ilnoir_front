BA;AACA,aAAA;AACA,QAAA,OAAA,EAAA,UADA;AAEA,QAAA,YAAA,EAAA,KAFA;AAGA,QAAA,MAAA,EAAA,SAHA;AAIA,QAAA,eAAA,YAAA,KAAA,QAAA,CAAA,UAAA,CAAA,CAJA;AAKA,QAAA,UAAA,EAAA;AALA,OAAA;AAOA;AAzBA,GA5GA;AAuIA,EAAA,KAAA,EAAA;AAvIA,CAAA","sourcesContent":["<template>\n<div style=\"padding: 16px;margin-bottom:16px;border-radius:8px\" :style=\"[brandColor.main, brandBgColor.white]\">\n\n  <!--작성자, 별점, 컨텍스트메뉴-->\n  <div class=\"flex-between\">\n\n    <div class=\"flex-align\">\n      <div style=\"margin-right:8px\" :style=\"bgCoverStyle(card.author_img)\"></div>\n      <div class=\"wrap size-12\">\n        <div class=\"flex-align\">\n          <div style=\"font-weight:500;margin-right:8px\">{{ card.author }}</div>\n          <label-user-grade :grade=\"card.author_grade\" :fontSize=\"11\"></label-user-grade>\n        </div>\n        <div style=\"color:#9b9b9b\">{{ dateFormat(card.created_time, 'ago') }}</div>\n      </div>\n    </div>\n    \n    <!--별점, 작성자, 작성일자, 옵션-->\n    <div>\n      <div class=\"flex-end\">\n        <rating :value=\"card.rate ? card.rate : 0\" :rateSize=\"16\" :disabled=\"true\"\n          style=\"margin-bottom:8px;\"\n          rateColor=\"#ffc351\"></rating>\n      </div>\n      <!--<div v-if=\"card.order_product&&card.order_product.order_product_option\">-->\n      <!--  <div class=\"size-16\" :style=\"brandColor.main\"-->\n      <!--    style=\"line-height:26px;\"-->\n      <!--    v-for=\"item in card.order_product.order_product_option\" :key=\"item.id\">-->\n      <!--    {{ computedOption(item) }}-->\n      <!--  </div>-->\n      <!--</div>-->\n      \n      <div class=\"flex-align size-12 weight-500\" v-if=\"card.self\">\n        <div style=\"padding:8px 16px;border-radius:4px;cursor:pointer\" \n          :style=\"[brandBgColor.main]\" @click=\"modify\">수정</div>\n          <div :style=\"[delStyle, brandColor.white]\" @click=\"del\">삭제</div>\n      </div>\n\n    </div>\n\n  </div>\n    \n  <!--후기 내용, 후기 이미지-->\n  <div class=\"grid-box\" style=\"margin-top:8px\">\n    <div :style=\"brandColor.main\">\n      <div v-if=\"!card.secret\">\n        <div class=\"size-16 weight-500 margin-bottom-4\" style=\"white-space:pre-wrap; line-height:18px;\">{{ card.title }}</div>\n      </div>\n      <div v-if=\"!card.secret\">\n        <div class=\"size-12\" style=\"white-space:pre-wrap; line-height:16px;\">{{ card.content }}</div>\n      </div>\n      <div v-else\n           class=\"flex-align size-18 secret\"\n           style=\"margin:10px 0 16px\"\n           :style=\"brandColor.help\">\n        <i class=\"material-icons size-16\"\n           style=\"margin-right:4px\">lock</i>\n        비밀글 입니다.\n      </div>\n      <div v-if=\"card.imgs && card.imgs.length>0\" style=\"margin-top:16px\">\n        <image-single :imgs=\"card.imgs\" :width=\"100\"></image-single>\n        <!--<div :style=\"reviewImgStyle(card.imgs[0])\"></div>-->\n      </div>\n    </div>\n  </div>\n\n    <!-- 답변 -->\n    <div style=\"margin: 16px 0 0 20px;padding:12px 12px 16px;border-radius:8px\" \n      :style=\"brandBgColor.main\" v-if=\"card.comment\">\n      <div class=\"flex-between margin-bottom-14\">\n        <div class=\"flex-align\">\n          <!--<div class=\"author-img\" style=\"margin-right:8px\"-->\n          <!--:style=\"bgCoverStyle(card.author_img)\"></div>-->\n          <div class=\"size-12\">\n            <div :style=\"brandColor.primary\">{{ card.answerer }}</div>\n            <div :style=\"brandColor.sub\">{{ dateFormat(card.comment_date, 'ago') }}</div>\n          </div>\n        </div>\n      </div>\n      <div v-if=\"!card.secret\">\n        <e-editor :html=\"card.comment\" class=\"size-12\" :style=\"brandColor.main\"></e-editor>\n      </div>\n      <div v-else\n              class=\"flex-align size-13 secret margin-bottom-8\"\n              :style=\"brandColor.help\">\n        <i class=\"material-icons size-14\"\n            style=\"margin-right:4px\">lock</i>\n        비밀글 입니다.\n      </div>\n    </div>\n\n\n\n\n    <!-- 후기 삭제 -->\n    <sweet-modal ref=\"delModal\" overlay-theme=\"dark\">\n      <modal @confirm=\"deleteReview\" @cancel=\"$refs.delModal.close()\">\n        <div slot=\"header\">후기 삭제</div>\n        <div slot=\"content\">후기를 삭제하시겠습니까?</div>\n      </modal>\n    </sweet-modal>\n\n  </div>\n</template>\n<script>\nimport LabelUserGrade from \"@/components/LaunchPack_Common/Common/LabelUserGrade\"\n\nexport default {\n  name: 'CardReviewPc',\n  components: {\n    LabelUserGrade\n  },\n  mixins: [\n\n  ],\n  props: {\n    card: {\n      type: Object,\n      required: true,\n    },\n    last: {\n      type: Boolean,\n      required: false,\n    },\n    userType: {\n      type: String,\n      required: false,\n    },\n    partnerId: {\n      type: Number,\n    },\n\n  },\n  data() {\n    return {\n      \"menu\": [\n        {\n          \"name\": \"수정\",\n          \"action\": \"modify\"\n        },\n        {\n          \"name\": \"삭제\",\n          \"action\": \"del\"\n        }\n      ]\n    }\n  },\n  created() {\n      if(this.userType) {\n        this.menu = [\n          {\n            name: '답변 남기기',\n            action: 'regComment'\n          }\n        ]\n      }\n    },\n  \n  methods: {\n    bgCoverStyle(img) {\n      if(!img || img === '') {\n        img = this.dummyImage.person;\n      }\n      return {\n        backgroundImage: `url(${img})`,\n        borderRadius: '50%',\n        backgroundSize: 'cover',\n        backgroundPosition: 'center',\n        width: '40px',\n        height: '40px'\n      }\n    },\n    regComment() {\n        this.routerPush(`review_comment_reg?product_id=${this.card.product_id}&review_id=${this.card.id}&partner_id=${this.user.partner_id}`);\n      },\n    computedOption(item) {\n      if(item.option.option_type===0) {\n        return `[기본옵션] ${item.option.name}`\n      }\n      if(item.option.option_type===1) {\n        return `[추가옵션] ${item.option.name}`\n      }\n    },\n    reviewImgStyle(img) {\n        let deco = {\n          border: '1px solid ' + this.getColor('기본 테두리선 색상'),\n          backgroundSize: 'cover',\n          backgroundImage: 'url('+ img +')',\n          marginTop: '8px',\n          marginRight: '6px',\n          width: '100px',\n          height: '100px'\n        };\n\n        return deco;\n      },\n    modify() {\n        this.routerPush(`review_reg?product_id=${this.card.product_id}&review_id=${this.card.id}&partner_id=${this.user.partner_id}`);\n      },\n    del() {\n        this.$refs.delModal.open();\n      },\n    deleteReview() {\n        this.$axios.delete(`user/${this.user.user_id}/product/${this.card.product_id}/review/${this.card.id}`).then(res=>{\n          if(res.status===200) {\n            this.toast('후기가 삭제되었습니다.');\n            this.$refs.delModal.close();\n            this.$emit('del', this.card.id);\n          }\n        })\n      },\n    clickProduct() {\n        this.routerPush(`product_detail?id=${this.card.product_id}`);\n      }\n  },\n  computed: {\n    ratingStyle() {\n        return {\n          margin: '0 !important'\n        }\n      },\n    prodImgStyle() {\n        return {\n          width: '40px',\n          height: '40px',\n        }\n      },\n    isMine() {\n        if(this.userType==='partner') return true;\n        else if(this.user.user_id===parseInt(this.card.author_id)) return true;\n        else return false;\n      },\n    delStyle() {\n        return {\n          padding: '8px 16px',\n          borderRadius: '4px',\n          cursor: 'pointer',\n          backgroundColor: `${this.getColor('보조 내용 색상')}`,\n          marginLeft: '8px'\n        }\n      }\n  },\n  watch: {\n    \n  }\n}\n</script>\n<style scoped>\n.answer-box {\n  padding: 16px;\n}\n\n.author-img {\n  width: 40px;\n  height: 40px;\n  border-radius: 50%;\n}\n\n.sub-text-margin  {\n  margin-right: 2px;\n}\n\n.review-content {\n  font-size: 14px;\n  font-weight: normal;\n  margin: 12px 0 18px;\n}\n\n.line {\n  height: 1px;\n  margin-top: 16px;\n  width: 100%;\n}\n</style>\n\n"],"sourceRoot":"src/components/Components/Card"}]}