 this.date = '%d-%02d-%02d'.format(date.year(), date.month() + 1, date.date());\n    }\n  },\n  methods: {\n    openPicker: function openPicker() {\n      if (this.disabled) return;\n      this.pickerOpened = true;\n    },\n    closePicker: function closePicker() {\n      this.pickerOpened = false;\n    },\n    //캘린더 데이터 생성\n    setCalendar: function setCalendar() {\n      // 캘린더 시작일\n      var day = moment('%d-%02d-01'.format(this.year, this.month)).startOf('month'); // 캘린더 마지막일\n\n      var end_day = moment('%d-%02d-01'.format(this.year, this.month)).endOf('month').format('MM-DD');\n      var dates = []; // 시작일 전 공백 채우기\n\n      for (var i = 0; i < day.day(); i++) {\n        dates.push({\n          clicked: false,\n          year: 0,\n          month: 0,\n          day: 0\n        });\n      }\n\n      for (var _i = 0; day.format('MM-DD') != end_day; _i++) {\n        if (_i > 0) day.add(1, 'days');\n        var clicked = false;\n\n        if (day.format('YYYY-MM-DD') === '%d-%02d-%02d'.format(this.selectedYear, this.selectedMonth, this.selectedDay)) {\n          clicked = true;\n        }\n\n        dates.push({\n          clicked: clicked,\n          year: day.year(),\n          month: day.month() + 1,\n          day: day.date()\n        });\n      }\n\n      this.dates = dates;\n    },\n    //이전 달\n    prev: function prev() {\n      var prevDt = moment('%d-%02d-01'.format(this.year, this.month)).subtract(1, 'months');\n      this.year = prevDt.year();\n      this.month = prevDt.month() + 1;\n      this.setCalendar();\n    },\n    //다음 달\n    next: function next() {\n      var nextDt = moment('%d-%02d-01'.format(this.year, this.month)).add(1, 'months');\n      this.year = nextDt.year();\n      this.month = nextDt.month() + 1;\n      this.setCalendar();\n    },\n    isNow: function isNow(date) {\n      var now = moment();\n\n      if (date.year === now.year() && date.month === now.month() + 1 && date.day === now.date()) {\n        return true;\n      } else {\n   