      }\r\n        else if ( !this.value ) {\r\n          style.backgroundColor = '#D5D5D5';\r\n          style.justifyContent = 'flex-end';\r\n          style.paddingRight = '6px';\r\n        }\r\n\r\n        return style;\r\n      },\r\n      pointerStyle() {\r\n        let style = {};\r\n\r\n        if ( this.value ) {\r\n          style.right = '1px';\r\n        }\r\n        else if ( !this.value ) {\r\n          style.right = '1px';\r\n          style.left = '1px';\r\n        }\r\n\r\n        return style;\r\n      }\r\n    },\r\n\r\n    watch: {\r\n      val(n) {\r\n        this.value = n;\r\n      },\r\n      value(n) {\r\n        this.$emit('update:val', n);\r\n      }\r\n    },\r\n\r\n    methods: {\r\n      toggleVal() {\r\n        this.value = !this.value;\r\n      }\r\n    }\r\n  }\r\n</script>\r\n\r\n<style lang=\"stylus\" type=\"stylus\" scoped>\r\n  .base-switch-input\r\n    display inline-flex\r\n    align-items center\r\n    justify-content center\r\n\r\n  .range-bar\r\n    position relative\r\n    border-radius 23px\r\n    cursor pointer\r\n    width 40px\r\n    height 20px\r\n    display flex\r\n    align-items center\r\n\r\n  .range-pointer\r\n    position absolute\r\n    top 1px\r\n    cursor pointer\r\n    background #fff\r\n    width 18px\r\n    height 18px\r\n    border-radius 25px\r\n\r\n  .on-text\r\n  .off-text\r\n    font-size 12px\r\n    font-weight bold\r\n  .on-text\r\n    color #fff\r\n  .off-text\r\n    color #8E8E8E\r\n</style>\r\n"]}]}